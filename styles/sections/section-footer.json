{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"blockTypes": ["core/group"],
	"slug": "section-footer",
	"title": "Footer",
	"styles": {
		"color": {
			"background": "var:preset|color|brand-2",
			"text": "var:preset|color|base-2"
		},
		"typography": {
			"fontSize": "var:preset|font-size|small"
		},
		"elements": {
			"heading":{
				"color": {
					"text": "var:preset|color|base"
				}
			},
			"link":{
				"color": {
					"text": "var:preset|color|brand-4"
				},
				"typography": {
					"textDecoration": "none"
				},
				":hover":{
					"color": {
						"text": "var:preset|color|base"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var:preset|color|base-2",
					"text": "var:preset|color|contrast-3"
				}
			}
		}
	}
}
